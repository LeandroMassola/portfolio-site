<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head.ejs')%>
    <link rel="stylesheet" href="/css/contact.css">
    <title>Contact</title>
</head>
<body>
    <%- include('../partials/navBar.ejs')%>
    <main class="main__contact">
        <h2 class="title__contact">Let's Talk!</h2>
        <form class="form__contact" action="" method="POST">
            <div class="cont-inputs__contact">
                <label class="labels__form" for="name">
                    Name: 
                </label>
                <input id="name" name="name" type="text" class="input-name__contact inputs__contact">
                
                <label class="labels__form" for="last-name">
                    Last Name: 
                </label>
                <input id="last-name" name="last-name" type="text" class="input-last-name__contact inputs__contact">
    
                <label class="labels__form" for="email">
                    Email:
                </label>
                <input id="email" name="email" type="text" class="input-email__contact inputs__contact">
    
                <label class="labels__form" for="subject">
                    Subject:
                </label>
                <input id="subject" name="subject" type="text" class="input-subject__contact inputs__contact">

                <label class="labels__form" for="message">
                    Message:
                </label>
                <textarea id="message" name="message" type="text" class="input-message__contact inputs__contact"></textarea>
            </div>
        </form>
    </main> 
    <%- include('../partials/footer.ejs')%>
</body>
</html>